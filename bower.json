{
  "name": "ssl-webclient",
  "dist": [
    "./dist/**"
  ],
  "ignore": [
    "**/.*"
  ],
  "dependencies": {
    "popper.js": "=1.12.9",
    "bootstrap": "=3.0.2",
    "font-awesome": "~4.0",
    "jscli": "vialink/jscli#v0.0.2",
    "github-fork-ribbon-css": "~0.1.0"
  },
  "exportsOverride": {
    "popper.js": {
      "js": "dist/umd/*.*"
    },
    "bootstrap": {
      "js": "dist/js/*",
      "css": "dist/css/*",
      "fonts": "dist/fonts/*"
    },
    "font-awesome": {
      "css": "css/*",
      "fonts": "fonts/*"
    },
    "jscli": {
      "js": "js/*.js",
      "css": "css/*.css"
    },
    "github-fork-ribbon-css": {
      "css": "*.css"
    }
  }
}
